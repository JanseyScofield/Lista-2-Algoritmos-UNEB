/*Fa√ßa um algoritmo que leia dois valores e escreva os dois em ordem
crescente.*/

#include <stdio.h>

int main(){
	int v1, v2;
	printf("Digite o valor 1: ");
	scanf("%d", &v1);
	printf("Agora digite o valor 2: ");
	scanf("%d", &v2);
	if(v1 < v2){
		printf("%d %d", v1, v2);
	} else {
		printf("%d %d", v2, v1);
	}
}
